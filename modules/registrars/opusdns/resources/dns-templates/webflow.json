{
	"id": "webflow",
	"name": "Webflow",
	"description": "DNS records for Webflow website hosting",
	"category": "hosting",
	"version": "1.0.0",
	"author": "OpusDNS",
	"documentation_url": "https://university.webflow.com/lesson/manually-connect-a-custom-domain",
	"supports_subdomain": true,
	"records": [
		{
			"type": "A",
			"name": "@",
			"ttl": 3600,
			"values": ["75.2.70.75"],
			"description": "Webflow A record"
		},
		{
			"type": "CNAME",
			"name": "www",
			"ttl": 3600,
			"values": ["proxy-ssl.webflow.com."],
			"description": "Webflow CNAME for www subdomain"
		}
	],
	"variables": []
}
