{
	"id": "fastmail",
	"name": "Fastmail",
	"description": "MX and SPF records for Fastmail email service",
	"category": "email",
	"version": "1.0.0",
	"author": "OpusDNS",
	"documentation_url": "https://www.fastmail.help/hc/en-us/articles/1500000280261",
	"supports_subdomain": false,
	"records": [
		{
			"type": "MX",
			"name": "@",
			"ttl": 3600,
			"values": [
				"10 in1-smtp.messagingengine.com.",
				"20 in2-smtp.messagingengine.com."
			],
			"description": "Fastmail MX records"
		},
		{
			"type": "TXT",
			"name": "@",
			"ttl": 3600,
			"values": ["\"v=spf1 include:spf.messagingengine.com ?all\""],
			"description": "SPF record for Fastmail"
		}
	],
	"variables": []
}
